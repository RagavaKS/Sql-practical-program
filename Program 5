select * from exe6;

select (fname||"||Iname) name from exe6 where salary > (select salary from eхеб where id=763);

select (fname||"||Iname) as name, salary, deptid,jodid from exe6 where jodid=(select jodid from exe6 where id=769);

select id, fname||' '||Iname from exe6 where salary > (select avg(salary) from exe6);

select * from exe6 where salary=(select salary from exe6 where fname='deepika');
